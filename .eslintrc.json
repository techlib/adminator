{
    "rules": {
        "space-before-blocks": "error",
        "semi": "off",
        "quotes": "off",
        "curly": "off",
        "object-curly-spacing": "off",
        "comma-dangle": "off",
        "array-bracket-spacing": "off",
        "no-extra-semi": "off",
        "react/jsx-quotes": "off",
        "react/jsx-uses-vars": "error",
        "react/wrap-multilines": "off"
    },
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "jquery": true
    }
}
